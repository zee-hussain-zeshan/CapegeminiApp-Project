<!--STUDENTS SHOWING BY MODULE -->

<p id="notice"><%= notice %></p>
<h3><u><%= @module.module_name%></u> sessions</h3>
<table align="center">
  <thead>
    <tr>
      <th>Date</th>
      <th>Module</th>
    </tr>
  </thead>

  <tbody>
  	 <% @sessions.each do |entry| %>
       <tr class="<%= cycle('list_line_odd', 'list_line_even') %>">
        <td><%= entry.date %></td>
        <td><%= entry.module_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

